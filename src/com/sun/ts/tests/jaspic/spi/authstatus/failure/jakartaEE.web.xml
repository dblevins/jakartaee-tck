<web-app version="5.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd">
  <display-name>soap_app</display-name>
  <servlet>
    <servlet-name>FailureHello</servlet-name>
    <servlet-class>com.sun.ts.tests.jaspic.spi.authstatus.failure.FailureHelloImpl</servlet-class>
    <load-on-startup>0</load-on-startup>
    <security-role-ref>
      <role-name>ADM</role-name>
      <role-link>Administrator</role-link>
    </security-role-ref>
  </servlet>
  <servlet-mapping>
    <servlet-name>FailureHello</servlet-name>
    <url-pattern>/FailureHello</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>54</session-timeout>
  </session-config>
  <security-role>
    <description>the administrator role</description>
    <role-name>Administrator</role-name>
  </security-role>
</web-app>
